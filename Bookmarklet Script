javascript:(function(){let button=document.createElement('button');button.innerHTML='Multiply Power';button.className='button';button.style.width='100px';button.style.backgroundColor='transparent';button.style.borderColor='transparent';button.onclick=calculate;document.querySelector("body > div.geofs-ui-bottom").appendChild(button);let slider=document.createElement('input');slider.type='range';slider.min=0.01;slider.max=50;slider.style.color="red";slider.step=0.01;slider.value=1;slider.style.width='300px';slider.style.transform="translate(0%, 25%)";document.querySelector("body > div.geofs-ui-bottom").appendChild(slider);let textDisplay=document.createElement('div');textDisplay.style.transform="translate(-5%, -30%)";textDisplay.style.color='green';textDisplay.style.fontSize="20px";textDisplay.style.fontWeight="550";textDisplay.innerHTML="1";document.querySelector("body > div.geofs-ui-center > div.geofs-ui-right > div.geofs-pads-container").appendChild(textDisplay);slider.oninput=function(){textDisplay.innerHTML=slider.value;};var i=0;function calculate(){let value=parseInt(slider.value);console.log(geofs.aircraft.instance.setup.zeroThrustAltitude=1e+162);var i=0;while(i<12){if(geofs.aircraft.instance.engines[i]!=undefined){geofs.aircraft.instance.engines[i].thrust=geofs.aircraft.instance.engines[i].thrust*value;}if(geofs.aircraft.instance.engines[i].afterBurnerThrust!=undefined){geofs.aircraft.instance.engines[i].afterBurnerThrust=geofs.aircraft.instance.engines[i].afterBurnerThrust*value;}if(geofs.aircraft.instance.engines[i].reverseThrust!=undefined){geofs.aircraft.instance.engines[i].reverseThrust=geofs.aircraft.instance.engines[i].reverseThrust*value;}i++;}}})();
